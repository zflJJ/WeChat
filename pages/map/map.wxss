/**index.wxss**/
page{
  position: relative;
  width: 100%;
  height: 100%;
}
.container{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
}
.search-head {
  width: 100%;
  height: 104rpx;
  background-color: #ffffff;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
}
.person-center{
width: 48rpx;
height: 48rpx;
}
.search-park{
  margin-left: 26rpx;
  width: 550rpx;
	height: 64rpx;
	background-color: #f5f5f5;
	border-radius: 10rpx;
	border: solid 1px #dbdbdb;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.search-logo{
  margin-left: 23rpx;
  width: 32rpx;
  height: 32rpx;
}
.map-wrap {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.note-wrap{
  margin-left: auto;
  width: 64rpx;
  height: 64rpx;
  box-sizing: border-box;
  padding-left: 26rpx;
  padding-top: 15rpx;
}
.note-wrap image{
  display: block;
  width: 40rpx;
  height: 34rpx;
}
#map {
  width: 100%;
  height: 100%;
}

.card-wrap {
  position: absolute;
  width: 100%;
  height: 310rpx;
  left: 0;
  bottom: 60rpx;
  padding-top: 50rpx;
  transition: all 500ms ease-in-out;
  box-sizing: border-box;
}

.let-btn {
  position: absolute;
  left: 10rpx;
  top: calc(50% - 29rpx);
  width: 58rpx;
  height: 58rpx;
}

.right-btn {
  position: absolute;
  right: 10rpx;
  top: calc(50% - 29rpx);
  width: 58rpx;
  height: 58rpx;
}

.card-up {
  position: absolute;
  left: 313rpx;
  top: 0;
  width: 124rpx;
  height: 50rpx;
}

.card {
  width: 590rpx;
  height: 260rpx;
  background-color: #fff;
  margin: 0 auto;
  border-radius: 25rpx;
  box-sizing: border-box;
}

.card-info {
  width: 590rpx;
  height: 160rpx;
  box-sizing: border-box;
  padding-top: 30rpx;
  display: flex;
}

.park-types {
  padding-left: 28rpx;
  box-sizing: border-box;
  width: 450rpx;
  height: 100%;
  background-color: #fff;
}

.parktypes-top {
  width: 100%;
  height: 34rpx;
  display: flex;
  align-items: center;
}

.park-name {
  font-size: 32rpx;
  color: #000;
  max-width: 260rpx;
  text-overflow: ellipsis;
}

.park-type {
  margin-left: 5rpx;
  padding: 3rpx 5rpx;
  font-size: 20rpx;
  color: #fff;
  background-color: #5ac8fa;
  border-radius: 3rpx;
}

.park-distance {
  font-size: 20rpx;
  color: #666;
  margin-left: auto;
  min-width: 90rpx;
}

.park-position {
  position: relative;
  margin-top: 30rpx;
  line-height: 40rpx;
  width: 100%;
  box-sizing: border-box;
  padding-left: 25rpx;
}

.position-icon {
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  top: 7rpx;
  left: 0;
}

.positon-text {
  font-size: 24rpx;
  color: #666;
  text-overflow: ellipsis;
}

.park-num {
  width: 140rpx;
  height: 130rpx;
}

.park-monery {
  width: 140rpx;
  text-align: center;
  align-items: center;
  display: flex;
  justify-content: center;
}

.park-fh {
  font-size: 28rpx;
  color: #d01c95;
}

.park-fee {
  font-size: 40rpx;
  color: #d01c95;
}

.park-wz {
  font-size: 40rpx;
  color: #d01c95;
  width: 140rpx;
  text-align: center;
}

.park-title {
  width: 140rpx;
  font-size: 22rpx;
  color: #666;
  text-align: center;
  margin-top: 15rpx;
}

.card-bottom {
  position: relative;
  width: 590rpx;
  height: 100rpx;
  display: flex;
  font-size: 32rpx;
}

.park-navigation {
  position: relative;
  background-color: #fff;
  height: 100rpx;
  box-sizing: border-box;
  flex: 1;
  overflow: hidden;
}
.na-bg {
  width: 590rpx;
  height: 3rpx;
  background-color: #666;
  left: 0;
  top: 0;
}
.na-text{
  width: 100%;
  height: 97rpx;
  color: #d01c95;
  text-align: center;
  line-height: 94rpx;
}
.park-appoint {
  flex: 1;
  height: 100rpx;
  background-color: #d01c95;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
  box-sizing: border-box;
}

.move-wrap {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}

.tab-wrap {
  width: 100%;
  height: 100rpx;
  display: flex;
  box-sizing: border-box;
  padding: 20rpx 0;
  background-color: #ffffff;
}

.tab-item {
  position: relative;
  width: 250rpx;
  height: 60rpx;
  line-height: 54rpx;
  font-size: 32rpx;
  color: #1a1a1a;
  text-align: center;
}

.tab-bottom {
  position: absolute;
  left: 85rpx;
  bottom: 0;
  width: 80rpx;
  height: 6rpx;
  background-color: #d01c95;
}

.tab-acitive {
  color: #d01c95;
}

.parking-list {
  width: 100%;
  height: calc(100% - 102rpx);
  background-color: #f5f5f5;
  padding-top: 2rpx;
  box-sizing: border-box;
}

.parking-item {
  position: relative;
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx;
  background-color: #ffffff;
  margin-bottom: 2rpx;
  display: flex;
  align-items: center;
}
.parkingitem-zz{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
}
.parkingitem-zzleft{
  width: 600rpx;
  height: 100%;
}
.parkingitem-zzright{
  width: 150rpx;
  height: 100%;
}
.parking-left{
  max-width: 600rpx;
}
.parking-typename{
  display: flex;
  align-items: center;
}
.parking-name{
  max-width: 430rpx;
  font-size: 32rpx;
  color: #000000;
  text-overflow: ellipsis;
}
.parking-type{
  margin-left: 8rpx;
  padding: 3rpx 5rpx;
  font-size: 20rpx;
  color: #ffffff;
  background-color: #5ac8fa;
  border-radius: 3rpx;
  box-sizing: border-box;
}
.parking-state{
  margin-top: 15rpx;
   display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #666666;
}
.prking-cznum{
  margin-left: 5rpx;
  color: #1a1a1a;
}
.parking-l{
  margin-left: 15rpx;
}
.parking-position{
  margin-top: 10rpx;
  position: relative;
  box-sizing: border-box;
  padding-left: 30rpx;
}
.parkingposition-icon{
  position: absolute;
  left: 0;
  top: 8rpx;
  width: 28rpx;
  height: 28rpx;
}
.paringposition-text{
  margin-left: 5rpx;
  font-size: 28rpx;
  color: #666666;
}
.parking-right{
  margin-left: auto;
  width: 120rpx;
  font-size: 24rpx;
  color: #1a1a1a;
  text-align: center;
  margin-bottom: 10rpx;
}
.parkingriging-y{
  margin: 0 auto;
  display: block;
  width: 64rpx;
  height: 64rpx;
}
.parkingriging-dh{
  margin: 0 auto;
  width: 64rpx;
  height: 64rpx;
  box-sizing: border-box;
  border-radius: 32rpx;
  border: 2rpx solid #d01c95;
}
.parkingriging-dh image{
   display: block;
  width: 30rpx;
  height: 30rpx;
  margin: 15rpx auto;
}
.parkingriging-d{
  line-height: 30rpx;
}
.note{
  position: absolute;
  left: 50%;
  top:60rpx;
  transform: translate(-50%,0);
  background-color: rgba(0, 0, 0, 0.8);
  height:30px;
  line-height: 30px;
  font-size: 13px;
  color: #ffffff;
  border-radius: 15px;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}
.note-content{
  min-width: 200px;
  letter-spacing:1px;
  padding: 0 8px;
  line-height: 30px;
  text-align: center;
}
.note-tc{
  width: 2px;
}
.no-data{
  width: 100%;
  line-height: 120rpx;
  text-align: center;
  color: #666666;
  font-size: 24rpx;
}
.cz-acitive{
  background-color: #ff9500;
}