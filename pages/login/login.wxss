.input-title{
  margin-top: 45rpx;
  padding-left: 118rpx;
  width: 100%;
  font-size: 26rpx;
  color: #666666;
  box-sizing: border-box;
}
.input-wrap{
  margin: 23rpx auto 0;
  width: 588rpx;
  height: 95rpx;
  border:  solid 1rpx #e6e6e6;
  border-radius:95rpx;
  box-sizing: border-box;
  display: flex;
  align-items:center;
  padding: 25rpx 36rpx 25rpx 45rpx
}
.input-icon{
  width: 45rpx;
  height: 45rpx;
}
.input-phone{
  padding-left: 20rpx;
  width: 465rpx;
  height: 26rpx;
  font-size: 26rpx;
  color: #333333;
}
.input-code{
  padding-left: 20rpx;
  width: 265rpx;
  height: 26rpx;
  font-size: 26rpx;
  color: #333333;
}
.code-btn{
  width: 168rpx;
	height: 50rpx;
	border-radius: 25rpx;
	border: solid 1rpx #d01c95;
	font-size: 26rpx;
	color: #d01c95;
  line-height: 48rpx;
  background-color: #ffffff;
  box-sizing: border-box;
  padding: 0;
}
.login-btn{
  margin-top: 80rpx;
  width: 392rpx;
	height: 80rpx;
	background-image: linear-gradient(0deg, 
		#d01c95 0%, 
		#800d93 100%);
	box-shadow: 0px 12rpx 24rpx 0rpx 
		rgba(227, 0, 155, 0.6);
    font-size: 32rpx;
    color: #ffffff;
    line-height: 80rpx;
    padding: 0;
    border-radius: 40rpx;
}